@prefix fhir: <http://hl7.org/fhir/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# - resource -------------------------------------------------------------------

<http://example.com/fhir/example/Patient/EEPatient> a fhir:Patient ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "EEPatient"] ; # 
  fhir:meta [
     fhir:profile ( [
       fhir:v "http://example.com/fhir/example/StructureDefinition/EUPatient"^^xsd:anyURI ;
       fhir:link <http://example.com/fhir/example/StructureDefinition/EUPatient>
     ] )
  ] ; # 
  fhir:text [
     fhir:status [ fhir:v "generated" ] ;
     fhir:div "<div xmlns=\"http://www.w3.org/1999/xhtml\"><p style=\"border: 1px #661aff solid; background-color: #e6e6ff; padding: 10px;\"><b>Anonymous Patient</b> female, DoB Unknown</p><hr/></div>"
  ] ; # 
  fhir:gender [ fhir:v "female"] . # 

